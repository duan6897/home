sername: '张三' }\r\n      ],\r\n      selection: false,\r\n      operation: true,\r\n      operationArr: [{ size: 'small', name: '编辑' }],\r\n\r\n      tableHeight: 200,\r\n      border: true,\r\n      maxHeight: 500,\r\n      tableHead: [{\r\n        label: '用户名',\r\n        prop: 'username'\r\n      }, {\r\n        label: '电话',\r\n        prop: 'tel'\r\n      }, {\r\n        label: '角色',\r\n        prop: 'userrole'\r\n      }],\r\n      permissionData: [],\r\n      title: \"用户权限\"\r\n    }\r\n  },\r\n  created () {\r\n    this.getRelation()\r\n  },\r\n  methods: {\r\n    clickButton (name, index, row) {\r\n      let _this = this\r\n      console.log('测试返回', name, index, row)\r\n      switch (name) {\r\n        case '编辑':\r\n          _this.$nextTick(() => {\r\n            _this.getUserPermissions(row.id)\r\n            _this.$refs.permissions.handleShow()\r\n          })\r\n          break\r\n        default:\r\n          break\r\n      }\r\n    },\r\n    // 获取列表数据\r\n    getRelation () {\r\n      let _this = this\r\n      let options = {\r\n        page: 1,\r\n        pageSize: 10\r\n      }\r\n      httprequest.get('/common/user/page', options, res => {\r\n        console.log('测试返回数据', res)\r\n        _this.dataTable = res.data.data.list\r\n      })\r\n    },\r\n    // 获取用户菜单详情\r\n    getUserPermissions (id) {\r\n      let _this = this\r\n      httprequest.get(`/sys/user/${id}`, {}, res => {\r\n        console.log('测试用户详情', res)\r\n        _this.permissionData = res.data.data.menuList.map(response => {\r\n          return response\r\n        })\r\n        console.log('测试这个用户_this.permissionData', _this.permissionData)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>"],"sourceRoot":"src/views/User"}]}