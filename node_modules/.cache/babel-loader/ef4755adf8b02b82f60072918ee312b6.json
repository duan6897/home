">\r\n        <div class=\"welcome\">欢迎登录</div>\r\n        <div class=\"loginDialog\">\r\n          <el-form :model=\"ruleForm\"\r\n                   status-icon\r\n                   :rules=\"rules\"\r\n                   ref=\"ruleForm\"\r\n                   class=\"demo-ruleForm\">\r\n            <el-form-item prop=\"username\">\r\n              <el-input type=\"text\"\r\n                        v-model=\"ruleForm.username\"\r\n                        autocomplete=\"off\">\r\n                <i slot=\"prefix\"\r\n                   class=\"el-input__icon el-icon-user\"></i></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"password\">\r\n              <el-input type=\"password\"\r\n                        v-model=\"ruleForm.password\"\r\n                        autocomplete=\"off\"\r\n                        show-password>\r\n                <i slot=\"prefix\"\r\n                   class=\"el-input__icon el-icon-lock\"></i></el-input>\r\n            </el-form-item>\r\n            <el-form-item style=\"display:flex;justify-content:center\">\r\n              <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n              <el-button type=\"primary\"\r\n                         @click=\"submitForm('ruleForm')\">登录</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n// import strEnc from '@/assets/utils/desUtil'\r\nimport httprequest from '@/assets/api/request'\r\nexport default {\r\n  name: 'login',\r\n  data () {\r\n    let validateName = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请输入用户名'));\r\n      } else {\r\n        if (this.ruleForm.checkPass !== '') {\r\n          this.$refs.ruleForm.validateField('checkPass');\r\n        }\r\n        callback();\r\n      }\r\n    }\r\n    let validatePass = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请输入密码'));\r\n      } else {\r\n        if (this.ruleForm.checkPass !== '') {\r\n          this.$refs.ruleForm.validateField('checkPass');\r\n        }\r\n        callback();\r\n      }\r\n    }\r\n    return {\r\n      ruleForm: {\r\n        password: '',\r\n        username: '',\r\n      },\r\n      rules: {\r\n        password: [\r\n          { validator: validatePass, trigger: 'blur' }\r\n        ],\r\n        username: [\r\n          { validator: validateName, trigger: 'blur' }\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submitForm (formName) {\r\n      let _this = this\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          let password = _this.changePwd(_this.ruleForm)\r\n\r\n          let options = {\r\n\r\n            \"password\": password,\r\n            \"userName\": _this.ruleForm.username\r\n\r\n          }\r\n          httprequest.post('/auth/login', options, res => {\r\n            console.log('测试res', res)\r\n            if (res.data.ok) {\r\n              this.$router.push({ path: '/homePage' })\r\n\r\n            }\r\n          })\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    resetForm (formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    changePwd (data) {\r\n      let loginNameChars = data.username.split(\"\");\r\n      let pwdChars = data.password.split(\"\");\r\n      let pwdLength = pwdChars.length;\r\n      let saltChars = [];\r\n      let half = parseInt(pwdLength / 2);\r\n      for (var i = 0; i < pwdLength; i++) {\r\n        var saltItemInt = loginNameChars[parseInt(Math.random() * loginNameChars.length)]\r\n          .charCodeAt(0) + parseInt(Math.random() * 1000);\r\n        pwdChars[i] = String.fromCharCode(pwdChars[i].charCodeAt(0) ^ saltItemInt);\r\n        saltChars[i] = String.fromCharCode(saltItemInt);\r\n      }\r\n      let pwdStr = pwdChars.join(\"\");\r\n      let saltStr = saltChars.join(\"\");\r\n      let resultStr = saltStr.substring(0, half) + pwdStr + saltStr.substring(half);\r\n\r\n      return resultStr;\r\n    }\r\n  },\r\n\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.demo-ruleForm {\r\n  width: 80%;\r\n}\r\n#login {\r\n  width: 100vw;\r\n  height: 100vh;\r\n  // background: linear-gradient(#606266, #38191e);\r\n  background: url(\"../../static/images/login.png\") no-repeat;\r\n  background-size: 100vw 100vh;\r\n  .title {\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 80px;\r\n    left: 10%;\r\n    color: royalblue;\r\n    font-size: 30px;\r\n    line-height: 80px;\r\n    font-weight: bold;\r\n  }\r\n  .loginDia {\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: center;\r\n    align-items: center;\r\n    .loginDialogBox {\r\n      border-radius: 10px;\r\n      background: #ffffff;\r\n      border: 1px solid #ff00ff;\r\n      .welcome {\r\n        line-height: 20px;\r\n        text-align: center;\r\n        padding-top: 20px;\r\n        color: #3077dd;\r\n        font-weight: bold;\r\n      }\r\n      .loginDialog {\r\n        width: 300px;\r\n        height: 300px;\r\n\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views/Login"}]}